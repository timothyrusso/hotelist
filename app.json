{
  "expo": {
    "name": "hotelist",
    "slug": "hotelist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/ui/assets/images/icon.png",
    "scheme": "hotelist",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/ui/assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#5672bc"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.app.hotelist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/ui/assets/images/icon.png",
        "backgroundColor": "#5672bc"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.app.hotelist"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/ui/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./src/ui/assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#5672bc"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
